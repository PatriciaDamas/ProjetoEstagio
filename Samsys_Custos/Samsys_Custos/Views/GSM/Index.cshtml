@model IEnumerable<Samsys_Custos.Data.GSM>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.numero)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.equipamento)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.numero)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.equipamento)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.id_gsm">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id_gsm">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.id_gsm">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
