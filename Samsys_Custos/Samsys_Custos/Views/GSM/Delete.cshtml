@model Samsys_Custos.Data.GSM

@{
    ViewData["Title"] = "Delete";
}
<br />
<center><h2>Eliminar GSM</h2></center>
<br />
<hr />
<br />
<div class="row">
    <div class="col-md-4">
    </div>
    <div class="col-md-4">
       
            <h6>Tem a certeza que pertende eliminar este GSM?</h6>
            <br />
            <dl class="dl-horizontal">
                <dt>
                    Número
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.numero)
                </dd>
                <dt>
                    Equipamento
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.equipamento)
                </dd>
            </dl>

            <form asp-action="Delete">
                <input type="hidden" asp-for="id_gsm" />
                <div class="row">
                    <div class="col-md-6">
                        <a class="btn btnVoltarEdit2 form-control" asp-action="Index">Voltar</a>
                        
                    </div>
                    <div class="col-md-6">
                        <input type="submit" value="Remover" class="btn btnSave form-control" />
                    </div>
                </div>
            </form>
            <br />
           
        
    </div>
    <div class="col-md-4">
    </div>

</div>
