
@{
    ViewData["Title"] = "Média Equipas";
    ViewBag.TitlePage = "Média de Custo por Equipas";
    ViewBag.Class = "fa-bar-chart";
}
<hr />
<h8><b>Você está aqui : </b>Análise de Custos - Média de Custo por Equipas</h8>
<hr />

<div class="row">
    <div class="col-lg-12">
        <div class="card mb-3">
            <div class="card-header">
                <p hidden id="cName">@ViewBag.Class</p>
                <i class="fa" id="boxIcon"></i> @ViewBag.TitlePage

            </div>
            <div class="card-body">
                <select class="form-control col-sm-4" asp-items="ViewBag.ano" id="select_ano_custos"></select>
                <span class="text-danger"></span><div id="chartEquipaMedia" style="width: 100%;height: 650px"></div>

                <br>
                <br>
            </div>
        </div>
    </div>
</div>



@section scripts {
    <script src="https://www.amcharts.com/lib/3/amcharts.js"></script>
    <script src="https://www.amcharts.com/lib/3/serial.js"></script>
    <script src="https://www.amcharts.com/lib/3/plugins/dataloader/dataloader.min.js"></script>
    <script src="~/js/chart_equipa_media.js"></script>
}