@model IEnumerable<Samsys_Custos.Data.FORNECEDOR>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.sugestao_custo)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.nome)
                </th>
            <th>
                @Html.DisplayNameFor(model => model.CATEGORIA)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.sugestao_custo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CATEGORIA.id_categoria)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.id_fornecedor">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id_fornecedor">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.id_fornecedor">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
