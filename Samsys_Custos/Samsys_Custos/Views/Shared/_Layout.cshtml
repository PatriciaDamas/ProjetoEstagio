                        <i class="fa fa-fw fa-list"></i>
                        <span class="nav-link-text">Gestão</span>
                    </a>
                    <ul class="sidenav-second-level collapse" id="collapseMulti">
                        <li>
                            <a asp-area="" asp-controller="ATRIBUICAO" asp-action="Index">Atribuições Geral</a>
                        </li>
                        <li>
                            <a asp-area="" asp-controller="ATRIBUICAO" asp-action="Viatura">Atribuições Viaturas</a>
                        </li>
                        <li>
                            <a asp-area="" asp-controller="ATRIBUICAO" asp-action="GSM">Atribuições GSM</a>
                        </li>
                        <li>
                            <a asp-area="" asp-controller="EQUIPA" asp-action="CustoColaborador">Colaboradores</a>
                        </li>
                        <li>
                            <a asp-area="" asp-controller="" asp-action="">Perfis</a>
                        </li>

                        <li>
                            <a asp-area="" asp-controller="" asp-action="">Fornecedores</a>
                        </li>

                    </ul>
                </li>



            </ul>
            <ul class="navbar-nav sidenav-toggler">
                <li class="nav-item">
                    <a class="nav-link text-center" id="sidenavToggler">
                        <i class="fa fa-fw fa-angle-left"></i>
                    </a>
                </li>
            </ul>
            <ul class="navbar-nav ml-auto">

                <li class="nav-item">
                    <form class="form-inline my-2 my-lg-0 mr-lg-2">
                        <div class="input-group">
                            <input class="form-control" type="text" placeholder="Search for...">
                            <span class="input-group-append">
                                <button class="btn btn-primary" type="button">
                                    <i class="fa fa-search"></i>
                                </button>
                            </span>
                        </div>
                    </form>
                </li>
                <li class="nav-item">
                    <a class="nav-link" data-toggle="modal" data-target="#exampleModal">
                        <i class="fa fa-fw fa-sign-out"></i>Logout

                    </a>
                </li>
            </ul>
            @await Html.PartialAsync("_LoginPartial")
        </div>
    </nav>




    <div class="content-wrapper">
        <partial name="_CookieConsentPartial" />
        <div class="container-fluid">
          
            <div class="card mb-3">
                <div class="card-header">
                    <p  hidden id="cName">@ViewBag.Class</p>
                    <i class="fa" id="boxIcon"></i> @ViewBag.TitlePage

                </div>
                <div class="card-body">
                        @RenderBody()
                </div>
                <div class="card-footer small text-muted">Updated yesterday at 11:59 PM</div>
            </div>
        </div>
    </div>
    <footer class="sticky-footer">
        <div class="container">
            <div class="text-center">
