@model Samsys_Custos.Data.ATRIBUICAO

@{
    ViewData["Title"] = "DeleteGSM";
}
<br />
<center><h2>Remover Atribuição de GSM</h2></center>
<br />
<hr />
<br />

<div class="row">
    <div class="col-md-4">
    </div>
    <div class="col-md-4">
        <center>
            <h6>Deseja remover esta atribuição?</h6>
        </center>
        <br />
        <dl class="dl-horizontal">
            <dt style="font-weight:bold;">
                GSM
            </dt>
            <dd>
                @Html.DisplayFor(model => model.GSM.numero)
            </dd>
            <dt style="font-weight:bold;">
                Equipamento
            </dt>
            <dd>
                @Html.DisplayFor(model => model.GSM.equipamento)
            </dd>
            <dt style="font-weight:bold;">
                Colaborador
            </dt>
            <dd >
                @Html.DisplayFor(model => model.UTILIZADOR.nome)
            </dd>
            <dt style="font-weight:bold;">
                Data Início
            </dt>
            <dd>
                @Html.DisplayFor(model => model.data_inicio)
            </dd>
            <dt style="font-weight:bold;">
                Data Final
            </dt>
            <dd>
                @Html.DisplayFor(model => model.data_fim)
            </dd>
        </dl>

        <form asp-action="DeleteGSM">
            <input type="hidden" asp-for="id_atribuicao" />
            <div class="row">
                <div class="col-md-5">
                    <input type="submit" value="Remover" class="btn btnCreate" />
                </div>
                <div class="col-md-1">

                </div>
                <div class="col-md-5">
                    <a class="btn btnVoltar" asp-action="GSM">Voltar</a>
                </div>
            </div>
           
        </form>
        <div class="col-md-4">
        </div>
    </div>
</div>
